<?xml version="1.0" encoding="UTF-8" ?>
<parameters>
	<parameter name="randomSeed"
		displayName="Default Random Seed" type="int" defaultValue="__NULL__"
		isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$IntConverter" />
		
	<!-- Manned Aircraft -->
	<parameter name="Manned_LateralMaxSpeedMPS"
		displayName="Manned Max Lateral Speed (m/s)"
		type="double" defaultValue="61.73" isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter"/>
	<parameter name="Manned_VerticalMaxSpeedMPS"
		displayName="Manned Max Vertical Speed (m/s)"
		type="double" defaultValue="7.62" isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter"/>
	<parameter name="Manned_RequiredVerticalSeparationM"
		displayName="Manned Required Vertical Separation (m)"
		type="double" defaultValue="304.8" isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter"/>
	<parameter name="Manned_LayersPer360"
		displayName="Manned Number of Layers per 360 deg"
		type="double" defaultValue="2" isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter"/>
	<parameter name="Manned_BaseAltitude"
		displayName="Manned Lowest Altitude (m)"
		type="double" defaultValue="304.8" isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter"/>
	
	<!-- UAVs -->
		<parameter name="UAV_LateralMaxSpeedMPS"
		displayName="UAV Max Lateral Speed (m/s)"
		type="double" defaultValue="15.43" isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter"/>
	<parameter name="UAV_VerticalMaxSpeedMPS"
		displayName="UAV Max Vertical Speed (m/s)"
		type="double" defaultValue="1.52" isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter"/>
	<parameter name="UAV_RequiredVerticalSeparationM"
		displayName="UAV Required Vertical Separation (m)"
		type="double" defaultValue="30.48" isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter"/>
	<parameter name="UAV_LayersPer360"
		displayName="UAV Layers per 360 deg"
		type="double" defaultValue="2" isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter"/>
	<parameter name="UAV_BaseAltitude"
		displayName="UAV Lowest Altitude (m)"
		type="double" defaultValue="152.4" isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter"/>
</parameters>


